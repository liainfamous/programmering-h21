
class Person {
    private String navn;
    private String personnummer;
    private int alder;

    public Person(String navn, String personnummer, int alder) {
        this.navn = navn;
        this.personnummer = personnummer;
        this.alder = alder;
    }

    public String toString() {
        return  "Person: \n" +
                "Navn: " + navn + "\n" +
                "Personnummer: " + personnummer + "\n" + 
                "Alder: " + alder + "\n";
    }
}